def biggest(aDict):
     '''
    aDict: A dictionary, where all the values are lists.

    returns: The key with the largest number of values associated with it
    '''
    # Your Code Here
    values = aDict.values()
    longest_key = max(values)
    result_key = ''
    for k in aDict:
        if aDict[k] == longest_key:
            result_key = k
    return (result_key)
